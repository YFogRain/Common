<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 签名 控件的自定义属性 -->
    <declare-styleable name="Signature_Common">
        <attr name="penMinWidth" format="dimension" />
        <attr name="penMaxWidth" format="dimension" />
        <attr name="penColor" format="color" />
        <attr name="velocityFilterWeight" format="float" />
        <attr name="clearOnDoubleClick" format="boolean" />

        <attr name="textAddColor" format="color|reference" />
        <attr name="textAddSize" format="dimension" />
        <attr name="isShowTextAdd" format="boolean" />
    </declare-styleable>
</resources>